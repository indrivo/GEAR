{
  "SystemConfig": {
    "MachineIdentifier": ".GR.Data.Stage",
    "EntryUri": "http://localhost:5000"
  },
  "ConnectionStrings": {
    "Provider": "Npgsql.EntityFrameworkCore.PostgreSQL",
    "ConnectionString": "Host=127.0.0.1;Port=5432;Username=postgres;Password=[password];Persist Security Info=true;Database=[Database name];MaxPoolSize=1000;"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Error",
      "System": "Information",
      "Microsoft": "Information"
    },
    "Seq": {
      "ServerUrl": "http://localhost:5341",
      "ApiKey": "1234567890",
      "MinimumLevel": "Trace",
      "LevelOverride": {
        "Microsoft": "Warning"
      }
    }
  },
  "IsConfigured": false,
  "WebClients": {
    "CORE": {
      "uri": "http://localhost:9099"
    },
    "Spa": {
      "uri": "http://localhost:9099"
    },
    "Xamarin": {
      "uri": "http://localhost:9099/xamarincallback"
    }
  },
  "BackupSettings": {
    "Enabled": false,
    "BackupFolder": "App_Name",
    "Interval": "24",
    "PgDumpPath": "C:\\Program Files\\PostgreSQL\\11\\bin\\pg_dump.exe",
    "Host": "localhost",
    "Port": "5432",
    "User": "postgres",
    "Password": "1111",
    "Database": "DbName",
    "FileExtension": "backup"
  },
  "EmailSettings": {
    "Enabled": false,
    "Host": "",
    "Port": 587,
    "Timeout": 5000,
    "EnableSsl": true,
    "NetworkCredential": {
      "Email": "",
      "Password": ""
    }
  },
  "RedisConnection": {
    "Host": "127.0.0.1",
    "Port": "6379"
  },
  "Sentry": {
    "Dsn": "",
    "IncludeRequestPayload": true,
    "SendDefaultPii": true,
    "MinimumBreadcrumbLevel": "Debug",
    "MinimumEventLevel": "Warning",
    "AttachStackTrace": true,
    "Debug": true,
    "DiagnosticsLevel": "Error"
  }
}