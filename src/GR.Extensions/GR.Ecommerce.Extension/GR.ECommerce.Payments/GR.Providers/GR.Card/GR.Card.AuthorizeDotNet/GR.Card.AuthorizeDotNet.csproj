<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Lupei Nicolae</Authors>
    <Company>Indrivo</Company>
    <Version>1.9.4</Version>
    <PackageIconUrl>https://i.ibb.co/KFYfF4D/indrivo-logo.jpg</PackageIconUrl>
    <Version>1.9.4</Version>
    <FileVersion>1.9.4</FileVersion>
    <AssemblyVersion>1.9.4</AssemblyVersion>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <NoWarn>$(NoWarn);1591</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="AppSettings\CardSettings.json" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="AppSettings\CardSettings.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\GR.Card.Abstractions\GR.Card.Abstractions.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="AuthorizeNET">
      <HintPath>Assemblies\AuthorizeNET.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
