<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>7.3</LangVersion>
    <Version>1.9.4</Version>
    <Company>Indrivo</Company>
    <Authors>Lupei Nicolae</Authors>
    <PackageIconUrl>https://i.ibb.co/KFYfF4D/indrivo-logo.jpg</PackageIconUrl>
    <PackageReleaseNotes>-Add in memory cache provider</PackageReleaseNotes>
    <FileVersion>1.9.4</FileVersion>
    <AssemblyVersion>1.9.4</AssemblyVersion>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <NoWarn>$(NoWarn);1591</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="AppSettings\CacheConfiguration.json" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="AppSettings\CacheConfiguration.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="AspNetCore.HealthChecks" Version="1.0.0" />
    <PackageReference Include="AspNetCore.HealthChecks.Redis" Version="2.2.1" />
    <PackageReference Include="Microsoft.Extensions.Caching.Redis" Version="2.2.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\GR.Cache.Abstractions\GR.Cache.Abstractions.csproj" />
  </ItemGroup>

</Project>
