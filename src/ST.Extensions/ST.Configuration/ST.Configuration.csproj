    <ProjectReference Include="..\ST.Processes.Extension\ST.Processes\ST.Procesess.csproj" />
    <ProjectReference Include="..\ST.Render.Extension\ST.PageRender.Abstractions\ST.PageRender.Abstractions.csproj" />
